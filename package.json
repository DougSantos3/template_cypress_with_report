{
  "name": "test_qa_api_ui",
  "version": "1.0.0",
  "description": "Test qa api and ui",
  "main": "cypress.config.js",
  "scripts": {
    "test:run": "cross-env NODE_ENV=$NODE_ENV TEST_TYPE=$TEST_TYPE BROWSER=$BROWSER cypress run --spec './cypress/e2e/$(TEST_TYPE)/tests/*.cy.js' --browser ${BROWSER:-chrome} --env allure=true",
    "test:open": "cypress open --browser ${BROWSER:-chrome}",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "allure:generate": "allure generate allure-results --clean",
    "view:report": "allure generate allure-results --clean && allure open",
    "server": "allure serve",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true"
  },
  "keywords": [],
  "author": "Douglas Martins Santos",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.32.0",
    "allure-cypress": "^3.0.9",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "cypress-postgres-10v-compatibility": "^1.0.0",
    "cypress-xpath": "^2.0.1",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "pg-promise": "^11.10.2",
    "prettier": "^3.4.2"
  }
}