{
  "name": "test_qa_api_ui",
  "version": "1.0.0",
  "description": "Test qa api and ui",
  "main": "cypress.config.js",
  "scripts": {
    "cy:open": "cypress open",
    "test:ui:dev": "cross-env NODE_ENV=dev TEST_TYPE=ui cypress run --spec './cypress/e2e/ui/tests/*.cy.js'",
    "test:api:dev": "cross-env NODE_ENV=dev TEST_TYPE=api cypress run --spec './cypress/e2e/api/tests/*.cy.js'",
    "test:ui:qa": "cross-env NODE_ENV=qa TEST_TYPE=ui cypress run --spec './cypress/e2e/ui/tests/*.cy.js'",
    "test:api:qa": "cross-env NODE_ENV=qa TEST_TYPE=api cypress run --spec './cypress/e2e/api/tests/*.cy.js'",
    "test:ui:prod": "cross-env NODE_ENV=prod TEST_TYPE=ui cypress run --spec './cypress/e2e/ui/tests/*.cy.js'",
    "test:api:prod": "cross-env NODE_ENV=prod TEST_TYPE=api cypress run --spec './cypress/e2e/api/tests/*.cy.js'",
    "allure:generate": "allure generate allure-results --clean",
    "view:report": "allure generate allure-results --clean && allure open",
    "server": "allure serve"
  },
  "keywords": [],
  "author": "Douglas Martins Santos",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.32.0",
    "allure-cypress": "^3.0.9",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "cypress-postgres-10v-compatibility": "^1.0.0",
    "cypress-xpath": "^2.0.1",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "pg-promise": "^11.10.2",
    "prettier": "^3.4.2"
  }
}